{
	"name": "Mediatate",
	"version": "0.1",
	"manifest version": "0",

	"description": "5 minute meditation",
	"permissions": ["storage"],

	"chrome_url_overrides": {                //set HTML page for new tabs 
		"newtab": "home.html"
	},

	"background": {
		"scripts": ["timer.js"],
		"persistent": false
	},

	"browser_action": {					
		"default_icon": {					 
			"16": "ensoicon16.png",  	
			"48": "ensoicon48.png",
			"128": "ensoicon128.png"
      }
    },

	"icons": {								// images on the extension management page, 
      "16": "ensoicon16.png",  				//  the permissions warning, and favicon.
      "48": "ensoicon48.png",
      "128": "ensoicon128.png"
    },
}